Host *
  ControlMaster auto
  ControlPath ~/.ssh/master-%r@%h:%p
  ControlPersist 10m
  ForwardAgent yes
  ServerAliveInterval 30
# BEGIN ANSIBLE MANAGED BLOCK
Host staging-bastion-host-0
HostName 34.197.188.90
User venky_iyer
AddKeysToAgent yes
UseKeychain yes
ForwardAgent yes
ControlMaster auto
ControlPath ~/.ansible/cp/ansible-ssh-%h-%p-%r
ControlPersist 10m
IdentityFile ~/.ssh/id_rsa

